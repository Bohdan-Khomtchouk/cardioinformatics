@article{NHLBI:2014:LossofFunction,
  title = {Loss-of-{{Function Mutations}} in {{{\emph{APOC3}}}}{\emph{,}} {{Triglycerides}}, and {{Coronary Disease}}},
  volume = {371},
  issn = {0028-4793, 1533-4406},
  doi = {10.1056/NEJMoa1307095},
  language = {en},
  number = {1},
  journal = {New England Journal of Medicine},
  author = {{The TG and HDL Working Group of the Exome Sequencing Project, National Heart, Lung, and Blood Institute}},
  month = jul,
  year = {2014},
  pages = {22-31},
  annote = {\section{Summary}

\begin{itemize}

\item Exome sequencing of 3734 participants, statistical tests for association of rare variants (individually, or in aggregation within a gene) with plasma triglyceride levels

\begin{itemize}

\item Aggregated variants within a gene: when variants are too rare to be analyzed individually with sufficient power, gene becomes the unit of association analysis (rather than variant).

\end{itemize}

\item Aggregate of rare mutations in APOC3 (encoding apolipoprotein C3) associated with lower plasma triglyceride levels. Mutations being aggregated include 1 nonsense, 2 splice-site, 1 missense.

\item Carriers have lower triglyceride levels, lower circulating levels of APOC3 and lower risk of coronary heart disease.

\item "Loss-of-function" is defined by the functional consequences of the variant: if it introduces a frameshift, pre-mature stop codon, breaks splice-sites, ... it is automatically loss-of-function. Missense variants are not conclusively LoF, but in this context, the missense variant A43T was associated with lower plasma triglyceride, thus "suggested to be loss-of-function"

\end{itemize}

~}
}

